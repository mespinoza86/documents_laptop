#!/bin/bash
realpath=`readlink -fn $0`
export OPENSSL_CONF=/home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/sysroots/x86_64-linux/usr/lib/ssl/openssl.cnf SSL_CERT_DIR=/home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/sysroots/x86_64-linux/usr/lib/ssl/certs SSL_CERT_FILE=/home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/sysroots/x86_64-linux/usr/lib/ssl/cert.pem OPENSSL_ENGINES=/home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/sysroots/x86_64-linux/usr/lib/ssl/engines
exec -a `dirname $realpath`/openssl `dirname $realpath`/openssl.real "$@"
