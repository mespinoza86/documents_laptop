	if [ -p /home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/work/cortexa9hf-neon-xilinx-linux-gnueabi/libgcc-xilinx/2016.4-r1/temp/fifo.12379 ] ; then
		printf "%b\0" "bbnote $*" > /home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/work/cortexa9hf-neon-xilinx-linux-gnueabi/libgcc-xilinx/2016.4-r1/temp/fifo.12379
	else
		echo "NOTE: $*"
