	if [ -p /home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/work/x86_64-linux/pigz-native/2.3.3-r0/temp/fifo.11188 ] ; then
		printf "%b\0" "bbnote $*" > /home/marco/Documents/tesis/others/petalinux/dma-mapped/build/tmp/work/x86_64-linux/pigz-native/2.3.3-r0/temp/fifo.11188
	else
		echo "NOTE: $*"
	fi
